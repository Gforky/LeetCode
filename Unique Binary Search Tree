public class Solution {
    public int numTrees(int n) {
        // Note: The Solution object is instantiated only once and is reused by each test case.
        double count = 1;
    for (double i = 2; i <= n; i++)        //Catalan Number Cn = (2n)!/((n+1)!*n!)
        count = (n+i)*count/i;
    return (int)count;
    }
}
